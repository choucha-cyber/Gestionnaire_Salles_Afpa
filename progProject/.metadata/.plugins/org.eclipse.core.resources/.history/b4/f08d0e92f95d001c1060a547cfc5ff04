package Model;

public class Identification {
	private String nomCreation;
	private String prenomCreation;
	private String emailCreation;
	private String passwordCreation;
	private String statut;
	private String emailLogin;
	private String passwordLogin;
	private int id;
	
	private static int count;
	
	public String getNomCreation() {
		return nomCreation;
	}
	public void setNomCreation(String nomCreation) {
		this.nomCreation = nomCreation;
	}
	public String getPrenomCreation() {
		return prenomCreation;
	}
	public void setPrenomCreation(String prenomCreation) {
		this.prenomCreation = prenomCreation;
	}
	public String getEmailCreation() {
		return emailCreation;
	}
	public void setEmailCreation(String emailCreation) {
		this.emailCreation = emailCreation;
	}
	public String getPasswordCreation() {
		return passwordCreation;
	}
	public void setPasswordCreation(String passwordCreation) {
		this.passwordCreation = passwordCreation;
	}
	public String getStatut() {
		return statut;
	}
	public void setStatut(String statut) {
		this.statut = statut;
	}
	public String getEmailLogin() {
		return emailLogin;
	}
	public void setEmailLogin(String emailLogin) {
		this.emailLogin = emailLogin;
	}
	public String getPasswordLogin() {
		return passwordLogin;
	}
	public void setPasswordLogin(String passwordLogin) {
		this.passwordLogin = passwordLogin;
	}
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	
	public Identification(String nomCreation, String prenomCreation, String emailCreation, String passwordCreation,
			String statut, String emailLogin, String passwordLogin) {
		super();
		this.id = ++count;
		this.nomCreation = nomCreation;
		this.prenomCreation = prenomCreation;
		this.emailCreation = emailCreation;
		this.passwordCreation = passwordCreation;
		this.statut = statut;
		this.emailLogin = emailLogin;
		this.passwordLogin = passwordLogin;
	}
	@Override
	public String toString() {
		return "Identification [nomCreation=" + nomCreation + ", prenomCreation=" + prenomCreation + ", emailCreation="
				+ emailCreation + ", passwordCreation=" + passwordCreation + ", statut=" + statut + ", emailLogin="
				+ emailLogin + ", passwordLogin=" + passwordLogin + ", id=" + id + "]";
	}
	
	
	


}
